package src2;

public class p_3 {
	public static void main(String[] args) {
		Car car = null;
		FireEngine fe = new FireEngine();
		FireEngine fe2 = null;
		FireEngine fe3 = null;

		car = fe;
		fe2 = (FireEngine)car;
		fe3 = (FireEngine)car;
		fe2.stop();
		fe3.drive();
	}
}

class Car {
	String color;
	int door;

	void drive() {
		System.out.println("Drive");

	}
	void stop() {
		System.out.println("stop!!!");

	}
}
class FireEngine extends Car{
	void water() {
		System.out.println("water!!!");
	}
}
